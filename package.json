{
  "name": "ethereum-boilerplate",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://eth-boilerplate.vercel.app/",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "deploypage": "gh-pages -d .next",
    "clean": "npx hardhat clean",
    "chain": "npx hardhat node",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run --network localhost scripts/deploy.js",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js",
    "deploy:fuji": "npx hardhat run --network fuji scripts/deploy.js",
    "deploy:ropsten": "npx hardhat run --network ropsten scripts/deploy.js",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deploy.js",
    "deploy:kovan": "npx hardhat run --network kovan scripts/deploy.js",
    "deploy:mainnet": "npx hardhat run --network mainnet scripts/deploy.js",
    "deploy:bsctest": "npx hardhat run --network kovan scripts/deploy.js",
    "deploy:bscmain": "npx hardhat run --network bscmain scripts/deploy.js",
    "test": "npx hardhat test"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.8",
    "@chakra-ui/react": "^2.2.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@moralisweb3/eslint-config": "^1.0.0",
    "@nomiclabs/hardhat-solhint": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.2",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@web3uikit/core": "^0.1.5",
    "@web3uikit/icons": "^0.1.5",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.9",
    "framer-motion": "^6",
    "moralis": "^2.0.3",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "wagmi": "^0.6.4"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.3",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.19.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "eslint": "8.22.0",
    "eslint-plugin-etc": "^2.0.2",
    "gh-pages": "^4.0.0",
    "hardhat": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
